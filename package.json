{
  "name": "matchsticks",
  "version": "1.0.0",
  "description": "react-powered 2-player nim-game",
  "main": "src/js/main.js",
  "scripts": {
    "build:webpack:prod": "NODE_ENV=production webpack --colors --config webpack.config.prod.js",
    "build:webpack:sockets": "webpack --colors --progress --watch --config webpack.config.devSockets",
    "build": "npm run build:webpack",
    "start": "npm run serve:sockets",
